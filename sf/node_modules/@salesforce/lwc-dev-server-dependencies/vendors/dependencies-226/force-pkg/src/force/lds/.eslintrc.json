{
  "extends": "eslint:recommended",
  "rules": {
    "lwc/no-compat-module-storage": "off",
    "lwc/no-compat-execute": "off",
    "lwc/no-inline-disable": "off",
    "lwc/no-wire-service": "off",
    "eslint-comments/no-unused-disable": "off",
    "no-unused-vars": "off",
    "no-cond-assign": "off",
    "no-redeclare": "off",
    "no-case-declarations": "off",
    "lwc/no-process-env": "off",
    "no-console": "off"
  }
}
