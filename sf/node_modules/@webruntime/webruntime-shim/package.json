{
    "name": "@webruntime/webruntime-shim",
    "license": "MIT",
    "version": "0.30.12",
    "description": "Client-side shim of Lightning Web Runtime",
    "files": [
        "dist/",
        "src/",
        "!**/__tests__",
        "!**/__mocks__"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "test": "jest --config jest.config.js",
        "test:unit": "jest --config jest-unit.config.js",
        "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
        "build:compat": "cross-env rollup -c ./scripts/rollup.config.compat.js",
        "build:shim": "cross-env rollup -c ./scripts/rollup.config.amd.js",
        "build:shim:es": "cross-env rollup -c ./scripts/rollup.config.es.js",
        "build": "yarn build:compat && yarn build:shim && yarn build:shim:es"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@lwc/jest-preset": "3.0.0-alpha.7",
        "@lwc/synthetic-shadow": "^1.2.2",
        "@webruntime/loader": "0.30.12",
        "@webruntime/rollup-plugins-dev": "0.30.12",
        "cross-env": "^5.2.0",
        "es-module-shims": "^0.4.6",
        "es5-proxy-compat": "^0.22.0",
        "jest": "^24.1.0",
        "rollup": "^1.25.1",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-compat": "^0.22.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^5.1.2",
        "whatwg-fetch": "^3.0.0"
    },
    "lwc": {
        "modules": [
            "@webruntime/loader"
        ]
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "gitHead": "f91998cb535573b8cf07f2a0ce7595c10ff466c1"
}
