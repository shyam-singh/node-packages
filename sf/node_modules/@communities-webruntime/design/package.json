        "@lwc/jest-preset": "3.0.0-alpha.7",
        "@lwc/module-resolver": "1.2.2",
        "@webruntime/connect-gen": "0.0.28",
        "@webruntime/rollup-plugins-dev": "0.30.12",
        "cross-env": "^5.2.0",
        "es5-proxy-compat": "^0.22.0",
        "gzip-size": "^5.0.0",
        "jest": "^24.1.0",
        "lwc": "1.2.2",
        "lwc-components-lightning": "^1.5.5-alpha",
        "path-to-regexp": "^6.0.0",
        "rollup": "^1.25.1",
        "rollup-plugin-alias": "^2.2.0",
        "rollup-plugin-babel": "^4.3.2",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-compat": "^0.22.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-replace": "^2.2.0",
        "rollup-plugin-terser": "^5.1.2",
        "whatwg-fetch": "^3.0.0"
    },
    "publishConfig": {
