{
    "name": "@communities-webruntime/design",
    "license": "MIT",
    "version": "0.30.12",
    "description": "Design-time libraries for Lightning Web Runtime",
    "files": [
        "dist/",
        "src/",
        "!**/__tests__",
        "!**/__mocks__"
    ],
    "scripts": {
        "clean": "rm -rf dist",
        "test": "jest --config jest.config.js",
        "test:unit": "jest --config jest-unit.config.js",
        "test:integration": "jest --config jest-integration.config.js",
        "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
        "build": "cross-env rollup -c ./scripts/rollup.config.designmode.js",
        "build:dev": "cross-env rollup -w -c ./scripts/rollup.config.designmode.js"
    },
    "devDependencies": {
        "@babel/core": "^7.5.5",
        "@communities-webruntime/common": "0.30.12",
